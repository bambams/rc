
[alias]
blame = blame -clnu
log.. = log -r "ancestor($1,$2)::$2 - ancestor($1,$2)"
html-diff = !hg-html-diff $@
in = in -Gv
log = log -Gv
out = out -Gv

[bookmarks]
track.current = True

[diff]
git = True

[eol]
only-consistent = False

[extensions]
#autopager = ~/src/autopager/autopager.py
bookmarks = 
bugzilla = 
children = 
churn = 
color = 
convert = 
eol = 
fetch = 
gpg = 
graphlog = 
hgk = 
highlight = 
#histedit = ~/src/histedit/hg_histedit.py
#keyword = 
mq = 
notify = 
#pager = 
parentrevspec = 
patchbomb = 
progress = 
purge = 
rebase = 
record = 
relink = 
schemes = 
#shelve = ~/src/hgshelve/hgshelve.py
transplant = 

[pager]
#[autopager]
attend = annotate, blame, bookmarks, branches, cat, diff, export, glog, grep, heads, help, in, incoming, log, merge, out, outgoing, qapplied, qdiff, qguard, qseries, qunapplied, resolve, status, summary, tags, tip
#pager = LESS='FSRX' less

[server]
validate = true

[ui]
merge = internal:merge
username = Brandon McCaig <bamccaig@gmail.com>

%include .hgrc.override

