[alias]
alog = log -Gv
blame = blame -clnu
log.. = log -r "::$2 - ::$1"
flog = log
html-diff = !hg-html-diff $@
in = in -Gv
out = out -Gv
qlog = log -Gvr qbase:qtip
reflog = log -Gv --hidden

[bookmarks]
track.current = True

[diff]
git = True

[eol]
only-consistent = False

[extensions]
#autopager = ~/src/autopager/autopager.py
bookmarks = 
bugzilla = 
children = 
churn = 
color = 
convert = 
debugshell = ~/src/hg/contrib/debugshell.py
eol = 
extdiff =
evolve = ~/src/evolve-main/hgext/evolve.py
fetch = 
gpg = 
graphlog = 
hgk = 
highlight = 
histedit =
#keyword = 
mq = 
notify = 
pager = 
parentrevspec = 
patchbomb = 
progress = 
purge = 
rebase = 
record = 
relink = 
schemes = 
shelve =
transplant = 

[mq]
keepchanges = true
secret = true

[pager]
#[autopager]
attend = alog, annotate, blame, bookmarks, branches, cat, diff, export,
       flog, glog, grep, heads, help, in, incoming, log, log.., merge,
       out, outgoing, qapplied, qdiff, qguard, qlog, qseries, qunapplied,
       reflog, resolve, status, summary, tags, tip

pager = LESS='FSRX' less

[server]
validate = true

[ui]
merge = internal:merge
username = Brandon McCaig <bamccaig@gmail.com>

#%include .hgrc.override

