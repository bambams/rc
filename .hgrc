[alias]
blame = blame -clnu
log.. = log -r "::$2 - ::$1"
flog = log
html-diff = !hg-html-diff $@
in = in -Gv
log = log -Gv
out = out -Gv
qlog = log -Gvr qbase:qtip

[bookmarks]
track.current = True

[diff]
git = True

[eol]
only-consistent = False

[extensions]
#autopager = ~/src/autopager/autopager.py
bookmarks = 
bugzilla = 
children = 
churn = 
color = 
convert = 
eol = 
extdiff =
fetch = 
gpg = 
graphlog = 
hgk = 
highlight = 
#histedit = ~/src/histedit/hg_histedit.py
#keyword = 
mq = 
notify = 
pager = 
parentrevspec = 
patchbomb = 
progress = 
purge = 
rebase = 
record = 
relink = 
schemes = 
#shelve = ~/src/hgshelve/hgshelve.py
transplant = 

[mq]
keepchanges = true
secret = true

[pager]
#[autopager]
attend = annotate, blame, bookmarks, branches, cat, diff, export, flog,
       glog, grep, heads, help, in, incoming, log, log.., merge, out,
       outgoing, qapplied, qdiff, qguard, qlog, qseries, qunapplied,
       resolve, status, summary, tags, tip
#pager = LESS='FSRX' less

[server]
validate = true

[ui]
merge = internal:merge
username = Brandon McCaig <bamccaig@gmail.com>

#%include .hgrc.override

