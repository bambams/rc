mktmux -- full-cs /bin/bash -c '"tmux source-file ~/.tmux/session.d/full-cs"'
mktmux -- social /bin/bash -c '"tmux source-file ~/.tmux/session.d/social"'
mktmux -- py /bin/bash -c '"tmux source-file ~/.tmux/session.d/py"'
mktmux -- scratch /bin/bash -c '"tmux source-file ~/.tmux/session.d/scratch"'
alias hgdev=py
