[alias]
    alog = log --all --graph
    amend = commit --amend --no-edit
    fall = fetch --all
    ff = merge --ff --ff-only
    fixup = commit --fixup
    squash = commit --squash
    staged = diff --cached
    start = checkout -tb
[core]
    filemode = false
    autocrlf = input
[log]
    decorate = short
[merge]
    ff = false
    log = 100
[push]
    default = tracking
[rebase]
    autosquash = true
[user]
    name = "Brandon McCaig"
    email = "bamccaig@gmail.com"
[color]
    diff = auto
    grep = auto
[pretty]
    subject = format:%s
